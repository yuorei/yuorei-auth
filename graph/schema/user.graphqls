scalar Upload

type CreateUserPayload {
		firstName: String
		lastName:  String
		email:     String
		username:  String!
		profileImageURL: String
}

input CreateUserInput {
		firstName: String
		lastName:  String
		email:     String
		username:  String!
		profileImage: Upload
}

type Mutation {
  createUser(input: CreateUserInput!): CreateUserPayload!
}
